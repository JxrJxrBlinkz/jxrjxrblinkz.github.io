<addon id="plugin.program.enigmawiz" name="[COLOR lime]Enigma GUI Wizard[/COLOR]" version="2.0.2" provider-name="[COLOR red]F[COLOR white]T[COLOR blue]G[/COLOR], Slam, Funsterbe">
